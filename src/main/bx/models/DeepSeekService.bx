class extends="OpenAIService"{

	static {
		chatURL = "https://api.deepseek.com/v1/chat/completions"
		imagesURL = "https://api.deepseek.com/v1/images/generations"
		embeddingsURL = "https://api.deepseek.com/v1/embeddings"
		settings = getModuleInfo( "bx-ai" ).settings
		defaultTimeout = 30
		defaultRole = "user"
		defaultModel = "deepseek-chat"
	}

	function invoke(
		required any messages,
		struct properties = {},
		string type = "chat",
		boolean verbose = false
	){
		return super.invoke( argumentCollection = arguments );
	}

}
